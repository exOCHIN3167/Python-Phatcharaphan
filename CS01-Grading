A = int(input("คะแนนเก็บของนักเรียน ไม่เกิน30คะแนน : "))
B = int(input("คะแนนสอบกลางภาคของนักเรียน ไม่เกิน30คะแนน : "))
C = int(input("คะแนนสอบปลายภาคของนักเรียน ไม่เกิน40 คะแนน : "))
D=A+B+C
if D <=40:
    print("F")
elif D <= 54:
    print("D")
elif D <=59:
    print("D+")
elif D <=64:
    print("C")
elif D<= 69:
    print("C+")
elif D<= 74:
    print("B")
elif D <=79:
    print("B+")
else:
    print("A")